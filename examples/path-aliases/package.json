  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^13.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.10",
    "vike": "0.4.168"
  },
  "type": "module"
}
