#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vvacla/repo/oss/vike-docs-ja/vike/node/cli/node_modules:/home/vvacla/repo/oss/vike-docs-ja/vike/node/node_modules:/home/vvacla/repo/oss/vike-docs-ja/vike/node_modules:/home/vvacla/repo/oss/vike-docs-ja/node_modules:/home/vvacla/repo/oss/node_modules:/home/vvacla/repo/node_modules:/home/vvacla/node_modules:/home/node_modules:/node_modules:/home/vvacla/repo/oss/vike-docs-ja/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vvacla/repo/oss/vike-docs-ja/vike/node/cli/node_modules:/home/vvacla/repo/oss/vike-docs-ja/vike/node/node_modules:/home/vvacla/repo/oss/vike-docs-ja/vike/node_modules:/home/vvacla/repo/oss/vike-docs-ja/node_modules:/home/vvacla/repo/oss/node_modules:/home/vvacla/repo/node_modules:/home/vvacla/node_modules:/home/node_modules:/node_modules:/home/vvacla/repo/oss/vike-docs-ja/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../vike/node/cli/bin-entry.js" "$@"
else
  exec node  "$basedir/../../../../vike/node/cli/bin-entry.js" "$@"
fi
